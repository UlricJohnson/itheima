package com.itheima;

import java.util.ArrayList;

public class Test4 {

	public static void main(String[] args) {
		ArrayList<Person> aList = new ArrayList<Person>();
		System.out.println("原集合：" + aList);

	}

	public static <T> void addArrayToList(T[] arr, ArrayList<T> aList) {
		for (T t : arr) {
			aList.add(t);
		}
	}
}
