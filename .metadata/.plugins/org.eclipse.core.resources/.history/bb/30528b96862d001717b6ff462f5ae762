package com.itheima;

public class TestSwitch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 0;
		switch (a) {
		case 1:
			a++;
			break;
		case 2:
			a++;
			break;
		default:
			a++;
//			break;
		case 3:
			a++;
			break;
		case 4:
			a++;
			break;
		case 5:
			a++;
			break;
		}
		System.out.println("a = " + a); // a = 1
	}

}
