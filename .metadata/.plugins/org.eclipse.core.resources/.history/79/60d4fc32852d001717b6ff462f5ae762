package com.itheima;

public class TestSwitch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 3;
		switch (a) {
		
		case 1:
			a++;
		case 2:
			a++;
		case 3:
			a++;
		case 4:
			a++;
		case 5:
			a++;
		default:
			a++;
			break;
		}
		System.out.println("a = " + a); // a = 7
	}

}
