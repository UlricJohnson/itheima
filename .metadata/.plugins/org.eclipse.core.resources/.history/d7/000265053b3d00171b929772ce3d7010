package com.itheima;

public class Test7 {

	public static void main(String[] args) {
		print(9, 9);
	}

	public static void print(int row, int col) {// 9,9

		for (int i = 0; i < col; i++) {
			for (int j = row - i - 1; j >= 0; j--) {// 控制打印前面的空格
				System.out.print(" ");
			}
			for (int j = 1; j <= i + 1; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
